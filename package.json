{
  "name": "ipc-express",
  "version": "0.3.2",
  "author": "Ren√© Heinen <rene.heinen@gmx.net>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Blackfaded/ipc-express.git"
  },
  "description": "This package enables you to use the electron ipc like an express application without the http overhead.",
  "license": "MIT",
  "keywords": [
    "ipc",
    "electron",
    "express"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/ipc-express.esm.js",
  "size-limit": [
    {
      "path": "dist/ipc-express.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ipc-express.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/express": "^4.17.13",
    "@types/uuid": "^8.3.4",
    "electron": "^18.0.1",
    "electron-mock-ipc": "^0.3.12",
    "express": "^4.17.3",
    "husky": "^7.0.4",
    "message-port-polyfill": "^0.2.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {}
}
